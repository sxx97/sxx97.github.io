#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules/esbuild/bin/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules/esbuild/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules/esbuild/bin/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules/esbuild/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.18.20/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
