#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules/vitepress/bin/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules/vitepress/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules/vitepress/bin/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules/vitepress/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/registry.npmmirror.com+vitepress@1.0.0-rc.24_@algolia+client-search@4.20.0_search-insights@2.9.0/node_modules:/Volumes/JingZao/code/sxx97.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
